puts  "Calculating the period..."
#----------------------------------------------------------
# determine Natural Period, Frequency & damping parameters for SDOF 

set  lamiga [eigen 10] 
for  {set i 0}	{$i<10}	{incr i 1}	{
     set lamigaa [lindex $lamiga $i];
     set omegaa  [expr pow($lamigaa,0.5)];
     set T($i)   [expr 2*3.1415926/$omegaa]; #####################################################################
     puts "[expr $i+1] th period is:$T($i)"; #####################################################################  
}

